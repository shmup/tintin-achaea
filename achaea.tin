#alias sl {show level}
#alias ll {ih}
#alias q {#if {%1} {quest %1 details}; #else quest log;}
#alias connect {#session achaea achaea.com 23;}
#alias reload  {#kill all; #read achaea.tin; #config {command echo} {on}}
#act {Password correct. Welcome to Achaea.} {#config {command echo} {on}}

#split 1 1
#config {repeat char}   {!}
#config {repeat enter}  {off}
#config {tintin char}   {#}
#config {command echo}  {off}
#config {speedwalk}     {off}
#config {verbatim char} {\}
#config {log}           {html}
#config {mccp}          {on}
#config {buffer size}   {1000000}
#config {history size}  {2000}
#config {wordwrap} on

#alias alert {#system {afplay assets/caw1.wav}}

#act {You cease your meditation having achieved full will and mana.} {
  alert
}

#act {Your mental prowess is already swollen to its maximum.} {
  alert
}

